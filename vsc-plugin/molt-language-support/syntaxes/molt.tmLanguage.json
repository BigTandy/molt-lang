{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Molt",
	"patterns": [
		{
			"name": "keyword.control.molt",
			"match": "\\b(let|eval|solve|def|graph)\\b"
		},
		{
			"name": "comment.line.number-sign.molt",
			"begin": "#",
			"end": "\\n"
		},
		{
			"name": "keyword.operator",
			"match": "\\b(>>=|>>=|<<=|\\\\/|!=|/=|<<|>=|<=|&&|/\\\\|\\\\|\\||,|=|\\(|\\)|\\{|\\{|:|\\+|-|\\*|/|%|\\^|⊇|⊃|⊆|⊂|>|<|&|∩|\\||∩|~|\\|)\\b"
		},
		{
			"name": "constant.numeric",
			"match": "-?([0-9]*.)[0-9]+"
		},
		{
			"name": "entity.name",
			"match": "\b[a-zA-Z_]+\b"
		}
		
	],
	"foldingStartMarker": "\\(|{",
	"foldingStopMarker": "}|\\)",
	"scopeName": "source.molt"
}